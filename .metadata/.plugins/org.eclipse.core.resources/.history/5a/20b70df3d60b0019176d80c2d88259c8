package dss.miguel.p2.modelo;

import javax.xml.bind.annotation.XmlRootElement;

//Dataobject de la aplicacion
@XmlRootElement
public class Usuario {
	private String id;
	private String name;
	private String email;
	
	public Usuario () {
		
	}
	
	public Usuario(String id, String name, String email) {
		this.id = id;
		this.name = name;
		this.email = email;
	}
}